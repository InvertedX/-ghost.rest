    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-dev-server": "3.1.10",
    "webpack-hot-middleware": "2.24.3"
  },
  "scripts": {
    "start": "npm run pack-dev && concurrently \"npm run start-server\" \"npm run start-electron\"",
    "pack-dev": "tsc src/main/index.ts -sourcemap --lib es6,dom,scripthost && webpack --target web --config webpack.config.js && webpack --config webpack.config.js  --mode development",
    "pack-prod": "tsc src/main/index.ts -sourcemap --lib es6,dom,scripthost && webpack --target web --config webpack.production.config.js && webpack --config webpack.production.config.js",
    "start-server": "cross-env NODE_ENV=development webpack-dev-server  --mode development",
    "start-electron": "cross-env NODE_ENV=development electron src/main/index --enable-logging --remote-debugging-port=9222 --debug-brk=5858",
    "build-debug": "npm run pack-dev && electron-builder --dir",
    "build-win": "npm run pack-prod && electron-builder --win",
    "build-mac": "npm run pack-prod && electron-builder --mac",
    "build-linux": "npm run pack-prod && electron-builder --linux",
    "build": "npm run build-debug",
    "asarlist": "asar list ./dist/win-unpacked/resources/app.asar"
  },
  "build": {
    "appId": "com.example.app",
    "productName": "Sample App",
    "copyright": "Copyright Â© year ${author}",
    "forceCodeSigning": false,
    "directories": {
      "buildResources": "build",
      "output": "electron"
    },
    "files": [
      "node_modules/**",
      "src/main/**/*.js",
      "dist/electron.html",
      "dist/electron.js",
      "dist/index.html",
      "dist/site.js",
      "static/**",
      "LICENSE"
    ],
    "asarUnpack": [
      "static/preload.js"
    ],
    "extraResources": [],
    "extraFiles": [],
    "mac": {},
    "mas": {},
    "dmg": {},
    "pkg": {},
    "win": {
      "target": "portable"
    },
    "nsis": {},
    "nsisWeb": {},
    "portable": {},
    "appx": {},
    "squirrelWindows": {}
  },
  "author": "Sarath kumar (@_sarath_kumar)",
  "license": "ISC"
}
