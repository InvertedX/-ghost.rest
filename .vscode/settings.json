{
    "editor.tabSize": 2,
    "files.eol": "\n",
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/*.js": {"when": "$(basename).ts"}
    },
    "files.associations": {
        "*.jlib": "javascript"
    },
    "eslint.enable": true
}